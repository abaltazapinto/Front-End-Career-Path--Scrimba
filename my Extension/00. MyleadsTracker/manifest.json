{
    "manifest_version": 3,
    "version": "1.0",
    "name": "Leads tracker",
    "description": "Save your important links",
    "action": {
        "default_popup": "index.html",
        "default_icon": "icon.png"
    },
    "permissions": [
        "storage", 
        "activeTab"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self'"
    }
}